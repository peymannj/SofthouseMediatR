{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": { "path": "logs/log.txt", "rollingInterval": "Day" }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName"],
    "Properties": {
      "ApplicationName": "SofthouseWorker"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Server=.\\SQLEXPRESS;Database=SofthouseMediatR;TrustServerCertificate=True;Trusted_Connection=True"
  },
  "RabbitMqSettings": {
    "HostName": "localhost",
    "Port": 5671,
    "Username": "guest",
    "Password": "guest"
  },
  "SmtpSettings": {
    "Server": "localhost",
    "Port": "2525",
    "UseSsl": false,
    "From": "info@mail.io",
    "Username": "guest",
    "Password": "guest"
  }
}
